{
  "env": {
    "es6": true
  },
  "extends": "airbnb",
  "parserOptions": {
    "ecmaVersion": 2015,
    "sourceType": "module"
  },
  "env": {
    "node": true,
    "mocha": true,
    "jasmine": true
  },
  "rules":{
    "class-methods-use-this":"off",
    "no-await-in-loop":"off",
    "no-continue":"off",
    "lines-between-class-members":"off"
  }
}